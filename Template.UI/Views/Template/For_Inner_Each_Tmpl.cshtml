@using Incoding.MvcContrib
@using Template.UI.Models
@using (var template = Html.Incoding().Template<CountryVm>())
{
    <ul>
        @using (var each = template.ForEach())
        {
            <li>
                @each.For(r => r.Title) 
                <br/>
                Cities:
                    @using (var innerEach = each.ForEach(r => r.Cities))
                    {
                        <span>@innerEach.For(r=>r.Name) ,</span>
                    } 
            </li>
        }
    </ul>
}